#!/bin/sh
LANG=C # required to read binary files
print() {
  out="$(while read -r p; do
    IFS=""; echo "$p"
  done <&0)"
  IFS=""; echo "$out"
}
case "$#" in
  0)
    print;;
  *)
    for i in "$@"; do
      [ -e "$i" ] && { print < "$i"; } || { echo "$i is missing or not a file..."; }
    done
  ;;
esac
